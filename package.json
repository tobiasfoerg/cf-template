{
	"name": "cf-template",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"typegen": "wrangler types && react-router typegen",
		"typecheck": "pnpm typegen && tsc -b",
		"add:comp": "pnpm justd add",
		"add:icon": "pnpm sly add lucide-icons",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"postinstall": "pnpm run typegen",
		"db:studio": "pnpm drizzle-kit studio",
		"db:migrations": "drizzle-kit generate",
		"db:migrations:apply": "wrangler d1 migrations apply DB --local",
		"auth:db:generate": "BETTER_AUTH_GENERATE=1 pnpm dlx @better-auth/cli generate --config ./src/lib/auth/.server/__generate.ts --output ./src/db/schema/auth-schema.ts --y",
		"test:browser": "vitest --workspace=vitest.workspace.ts"
	},
	"dependencies": {
		"@conform-to/react": "^1.2.2",
		"@epic-web/client-hints": "^1.3.5",
		"@nanostores/react": "^0.8.4",
		"@react-router/cloudflare": "^7.3.0",
		"better-auth": "^1.2.4",
		"conform-to-valibot": "^1.14.3",
		"dayjs": "^1.11.13",
		"drizzle-orm": "1.0.0-beta.1-fd5d1e8",
		"es-toolkit": "^1.33.0",
		"hono": "^4.7.4",
		"isbot": "^5.1.23",
		"justd-icons": "^1.10.25",
		"nanostores": "^0.11.4",
		"neverthrow": "^8.2.0",
		"react": "^19.0.0",
		"react-aria": "^3.38.1",
		"react-aria-components": "^1.7.1",
		"react-dom": "^19.0.0",
		"react-router": "^7.3.0",
		"react-use": "^17.6.0",
		"ua-parser-js": "^2.0.2",
		"valibot": "^1.0.0-rc.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@cloudflare/vite-plugin": "^0.1.11",
		"@libsql/client": "^0.14.0",
		"@react-router/dev": "^7.3.0",
		"@react-router/fs-routes": "^7.3.0",
		"@react-types/shared": "^3.28.0",
		"@sly-cli/sly": "^1.14.0",
		"@standard-schema/spec": "^1.0.0",
		"@tailwindcss/vite": "^4.0.14",
		"@types/node": "^22.13.10",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/browser": "^3.0.8",
		"clsx": "^2.1.1",
		"drizzle-kit": "1.0.0-beta.1-af4c434",
		"justd-cli": "^2.7.1",
		"msw": "^2.7.3",
		"playwright": "^1.51.0",
		"react-stately": "^3.36.1",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.14",
		"tailwindcss-animate": "^1.0.7",
		"type-fest": "^4.37.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.2",
		"vite-plugin-icons-spritesheet": "^3.0.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.8",
		"vitest-browser-react": "^0.1.1",
		"wrangler": "^4.0.0"
	},
	"packageManager": "pnpm@10.6.2",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild",
			"msw",
			"sharp",
			"workerd"
		],
		"patchedDependencies": {
			"@react-aria/utils": "patches/@react-aria__utils.patch"
		}
	}
}
