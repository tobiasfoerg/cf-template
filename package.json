{
	"name": "cf-template",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "pnpm build && wrangler dev",
		"typegen": "wrangler types && react-router typegen",
		"typecheck": "pnpm typegen && tsc",
		"add:comp": "pnpm justd add",
		"add:icon": "pnpm sly add lucide-icons",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"postinstall": "pnpm run typegen",
		"db:studio": "pnpm drizzle-kit studio",
		"db:migrations": "drizzle-kit generate",
		"db:migrations:apply": "wrangler d1 migrations apply DB --local"
	},
	"dependencies": {
		"@conform-to/react": "^1.2.2",
		"@react-router/cloudflare": "^7.2.0-pre.1",
		"@ryanflorence/async-provider": "^0.0.1",
		"better-auth": "^1.1.18",
		"conform-to-valibot": "^1.14.2",
		"drizzle-orm": "^0.39.3",
		"hono": "^4.7.1",
		"isbot": "^5.1.22",
		"justd-icons": "^1.10.25",
		"nanostores": "^0.11.4",
		"picoquery": "^2.4.0",
		"react": "^19.0.0",
		"react-aria": "^3.37.0",
		"react-aria-components": "^1.6.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.2.0-pre.1",
		"valibot": "^1.0.0-rc.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@cloudflare/vite-plugin": "^0.1.1",
		"@cloudflare/workers-types": "^4.20250214.0",
		"@libsql/client": "^0.14.0",
		"@react-router/dev": "^7.2.0-pre.1",
		"@react-router/fs-routes": "^7.2.0-pre.1",
		"@sly-cli/sly": "^1.14.0",
		"@tailwindcss/vite": "^4.0.6",
		"@types/node": "^22.13.4",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.4",
		"justd-cli": "^2.4.5",
		"tailwind-merge": "^3.0.1",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^4.0.6",
		"tailwindcss-animate": "^1.0.7",
		"type-fest": "^4.34.1",
		"typescript": "^5.7.3",
		"vite": "^6.1.0",
		"vite-plugin-icons-spritesheet": "^3.0.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.5",
		"wrangler": "^3.109.1"
	},
	"packageManager": "pnpm@10.4.0",
	"pnpm": {
		"patchedDependencies": {
			"@ryanflorence/async-provider": "patches/@ryanflorence__async-provider.patch"
		},
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "sharp", "workerd"]
	}
}
